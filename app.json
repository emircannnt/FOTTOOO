    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Galerinizdeki karmaşayı çözmemiz için okuma izni vermelisiniz.",
          "savePhotosPermission": "Silme işlemleri için medya erişim izni gereklidir."
        }
      ]
    ]
  }
}
